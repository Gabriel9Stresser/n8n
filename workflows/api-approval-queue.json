        "combinationMode": "multiplex"
      },
      "type": "n8n-nodes-base.merge",
      "typeVersion": 3,
      "position": [380, 300],
      "id": "b2c3d4e5-0009-4000-8000-000000000009",
      "name": "Merge"
    },
    {
      "parameters": {
        "jsCode": "const base = $env.N8N_BASE_URL || 'http://localhost:5678';\nconst baseUrl = String(base).replace(/\\/$/, '');\nconst all = $input.all();\nconst items = all.map((inputItem) => {\n  const raw = inputItem.json;\n  const execId = raw.id;\n  const resp = raw.body ?? raw.data ?? raw;\n  const data = (resp && typeof resp === 'object' && resp.data !== undefined ? resp.data : resp) || {};\n  const resultData = data.resultData || {};\n  const runData = resultData.runData || {};\n  let stepName = null;\n  for (const nodeRuns of Object.values(runData)) {\n    const runs = Array.isArray(nodeRuns) ? nodeRuns : [];\n    const firstRun = runs[0];\n    if (!firstRun?.data?.main) continue;\n    const mainData = firstRun.data.main;\n    const firstInput = mainData[0]?.[0]?.json;\n    if (firstInput && typeof firstInput.stepName === 'string') {\n      stepName = firstInput.stepName;\n      break;\n    }\n  }\n  const id = execId || data.id;\n  const wf = data['workflowData'];\n  const workflowName = (wf && typeof wf === 'object' && wf.name) ? wf.name : 'Approval Sub-Workflow';\n  return {\n    id: id,\n    workflowName: workflowName,\n    stepName: stepName,\n    waitUrl: id ? `${baseUrl}/webhook-waiting/${id}` : null,\n    startedAt: data.startedAt,\n    status: data.status,\n  };\n});\nreturn [{ json: { items } }];\n"
      },
      "type": "n8n-nodes-base.code",
