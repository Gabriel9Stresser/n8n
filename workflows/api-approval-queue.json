      "name": "GET Executions"
    },
    {
      "parameters": {
        "jsCode": "const input = $input.first().json;\ntry {\n  if (!input || typeof input !== 'object') {\n    return [{ json: { items: [], error: 'Resposta inválida da API n8n' } }];\n  }\n  if (input.message && !Array.isArray(input.data)) {\n    return [{ json: { items: [], error: `API n8n: ${input.message}. Configure N8N_API_KEY e N8N_BASE_URL no ambiente.` } }];\n  }\n  const data = input.data ?? input;\n  const list = Array.isArray(data) ? data : [];\n  const approval = list;\n  if (approval.length === 0) {\n    return [{ json: { items: [] } }];\n  }\n  return approval.map(e => ({ json: { id: e.id, workflowId: e.workflowId } }));\n} catch (e) {\n  return [{ json: { items: [], error: (e && e.message) || 'Erro ao processar' } }];\n}"
      },
      "type": "n8n-nodes-base.code",
      "typeVersion": 2,
